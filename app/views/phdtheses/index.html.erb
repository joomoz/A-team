<p id="notice"><%= notice %></p>

<h1>Listing Phdtheses</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Author</th>
      <th>Title</th>
      <th>Journal</th>
      <th>Year</th>
      <th>Volume</th>
      <th>Number</th>
      <th>Pages</th>
      <th>Month</th>
      <th>Note</th>
      <th>Publisher</th>
      <th>Series</th>
      <th>Address</th>
      <th>Edition</th>
      <th>Booktitle</th>
      <th>Editor</th>
      <th>Organization</th>
      <th>Entry type</th>
      <th>Annote</th>
      <th>Chapter</th>
      <th>Crossref</th>
      <th>Howpublished</th>
      <th>Institution</th>
      <th>School</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phdtheses.each do |phdthesis| %>
      <tr>
        <td><%= phdthesis.key %></td>
        <td><%= phdthesis.author %></td>
        <td><%= phdthesis.title %></td>
        <td><%= phdthesis.journal %></td>
        <td><%= phdthesis.year %></td>
        <td><%= phdthesis.volume %></td>
        <td><%= phdthesis.number %></td>
        <td><%= phdthesis.pages %></td>
        <td><%= phdthesis.month %></td>
        <td><%= phdthesis.note %></td>
        <td><%= phdthesis.publisher %></td>
        <td><%= phdthesis.series %></td>
        <td><%= phdthesis.address %></td>
        <td><%= phdthesis.edition %></td>
        <td><%= phdthesis.booktitle %></td>
        <td><%= phdthesis.editor %></td>
        <td><%= phdthesis.organization %></td>
        <td><%= phdthesis.entry_type %></td>
        <td><%= phdthesis.annote %></td>
        <td><%= phdthesis.chapter %></td>
        <td><%= phdthesis.crossref %></td>
        <td><%= phdthesis.howpublished %></td>
        <td><%= phdthesis.institution %></td>
        <td><%= phdthesis.school %></td>
        <td><%= phdthesis.type %></td>
        <td><%= link_to 'Show', phdthesis %></td>
        <td><%= link_to 'Edit', edit_phdthesis_path(phdthesis) %></td>
        <td><%= link_to 'Destroy', phdthesis, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

