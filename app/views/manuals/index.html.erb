<p id="notice"><%= notice %></p>

<h1>Listing Manuals</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Author</th>
      <th>Title</th>
      <th>Journal</th>
      <th>Year</th>
      <th>Volume</th>
      <th>Number</th>
      <th>Pages</th>
      <th>Month</th>
      <th>Note</th>
      <th>Publisher</th>
      <th>Series</th>
      <th>Address</th>
      <th>Edition</th>
      <th>Booktitle</th>
      <th>Editor</th>
      <th>Organization</th>
      <th>Entry type</th>
      <th>Annote</th>
      <th>Chapter</th>
      <th>Crossref</th>
      <th>Howpublished</th>
      <th>Institution</th>
      <th>School</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manuals.each do |manual| %>
      <tr>
        <td><%= manual.key %></td>
        <td><%= manual.author %></td>
        <td><%= manual.title %></td>
        <td><%= manual.journal %></td>
        <td><%= manual.year %></td>
        <td><%= manual.volume %></td>
        <td><%= manual.number %></td>
        <td><%= manual.pages %></td>
        <td><%= manual.month %></td>
        <td><%= manual.note %></td>
        <td><%= manual.publisher %></td>
        <td><%= manual.series %></td>
        <td><%= manual.address %></td>
        <td><%= manual.edition %></td>
        <td><%= manual.booktitle %></td>
        <td><%= manual.editor %></td>
        <td><%= manual.organization %></td>
        <td><%= manual.entry_type %></td>
        <td><%= manual.annote %></td>
        <td><%= manual.chapter %></td>
        <td><%= manual.crossref %></td>
        <td><%= manual.howpublished %></td>
        <td><%= manual.institution %></td>
        <td><%= manual.school %></td>
        <td><%= manual.type %></td>
        <td><%= link_to 'Show', manual %></td>
        <td><%= link_to 'Edit', edit_manual_path(manual) %></td>
        <td><%= link_to 'Destroy', manual, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manual', new_manual_path %>
