<p id="notice"><%= notice %></p>

<h1>Listing Techreports</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Author</th>
      <th>Title</th>
      <th>Journal</th>
      <th>Year</th>
      <th>Volume</th>
      <th>Number</th>
      <th>Pages</th>
      <th>Month</th>
      <th>Note</th>
      <th>Publisher</th>
      <th>Series</th>
      <th>Address</th>
      <th>Edition</th>
      <th>Booktitle</th>
      <th>Editor</th>
      <th>Organization</th>
      <th>Entry type</th>
      <th>Annote</th>
      <th>Chapter</th>
      <th>Crossref</th>
      <th>Howpublished</th>
      <th>Institution</th>
      <th>School</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @techreports.each do |techreport| %>
      <tr>
        <td><%= techreport.key %></td>
        <td><%= techreport.author %></td>
        <td><%= techreport.title %></td>
        <td><%= techreport.journal %></td>
        <td><%= techreport.year %></td>
        <td><%= techreport.volume %></td>
        <td><%= techreport.number %></td>
        <td><%= techreport.pages %></td>
        <td><%= techreport.month %></td>
        <td><%= techreport.note %></td>
        <td><%= techreport.publisher %></td>
        <td><%= techreport.series %></td>
        <td><%= techreport.address %></td>
        <td><%= techreport.edition %></td>
        <td><%= techreport.booktitle %></td>
        <td><%= techreport.editor %></td>
        <td><%= techreport.organization %></td>
        <td><%= techreport.entry_type %></td>
        <td><%= techreport.annote %></td>
        <td><%= techreport.chapter %></td>
        <td><%= techreport.crossref %></td>
        <td><%= techreport.howpublished %></td>
        <td><%= techreport.institution %></td>
        <td><%= techreport.school %></td>
        <td><%= techreport.type %></td>
        <td><%= link_to 'Show', techreport %></td>
        <td><%= link_to 'Edit', edit_techreport_path(techreport) %></td>
        <td><%= link_to 'Destroy', techreport, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Techreport', new_techreport_path %>
