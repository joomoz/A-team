<p id="notice"><%= notice %></p>

<h1>Listing Booklets</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Author</th>
      <th>Title</th>
      <th>Journal</th>
      <th>Year</th>
      <th>Volume</th>
      <th>Number</th>
      <th>Pages</th>
      <th>Month</th>
      <th>Note</th>
      <th>Publisher</th>
      <th>Series</th>
      <th>Address</th>
      <th>Edition</th>
      <th>Booktitle</th>
      <th>Editor</th>
      <th>Organization</th>
      <th>Entry type</th>
      <th>Annote</th>
      <th>Chapter</th>
      <th>Crossref</th>
      <th>Howpublished</th>
      <th>Institution</th>
      <th>School</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @booklets.each do |booklet| %>
      <tr>
        <td><%= booklet.key %></td>
        <td><%= booklet.author %></td>
        <td><%= booklet.title %></td>
        <td><%= booklet.journal %></td>
        <td><%= booklet.year %></td>
        <td><%= booklet.volume %></td>
        <td><%= booklet.number %></td>
        <td><%= booklet.pages %></td>
        <td><%= booklet.month %></td>
        <td><%= booklet.note %></td>
        <td><%= booklet.publisher %></td>
        <td><%= booklet.series %></td>
        <td><%= booklet.address %></td>
        <td><%= booklet.edition %></td>
        <td><%= booklet.booktitle %></td>
        <td><%= booklet.editor %></td>
        <td><%= booklet.organization %></td>
        <td><%= booklet.entry_type %></td>
        <td><%= booklet.annote %></td>
        <td><%= booklet.chapter %></td>
        <td><%= booklet.crossref %></td>
        <td><%= booklet.howpublished %></td>
        <td><%= booklet.institution %></td>
        <td><%= booklet.school %></td>
        <td><%= booklet.type %></td>
        <td><%= link_to 'Show', booklet %></td>
        <td><%= link_to 'Edit', edit_booklet_path(booklet) %></td>
        <td><%= link_to 'Destroy', booklet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Booklet', new_booklet_path %>
